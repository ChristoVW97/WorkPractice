@model List<Employee>

<h1>Employee List</h1>
<table class="table table-striped table-border">
    <thead>
        <tr>
            <th>
                Employee Name
            </th>
            <th>
                Display Order
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(u=>u.DisplayOrder))
        {
            <tr class="table-default">

                <td>@obj.Name</td>
                <td>
                    @obj.DisplayOrder
                </td>
            </tr>
        }
    </tbody>
</table>